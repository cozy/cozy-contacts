{
  "name": "cozy-contacts",
  "version": "0.4.0",
  "scripts": {
    "precommit": "pretty-quick --staged",
    "clean": "rm -rf build/*",
    "tx": "tx pull --all --no-interactive || true",
    "format": "prettier --write '{src,test}/**/*.{js,jsx}'",
    "lint": "npm-run-all --parallel 'lint:*'",
    "lint:js": "eslint '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "npm-run-all --serial clean lint tx",
    "build": "NODE_ENV=browser:production cozy-scripts build",
    "prewatch": "NODE_ENV=browser:development npm-run-all --serial clean",
    "watch": "cozy-scripts watch",
    "watch:standalone": "NODE_ENV=browser:development cozy-scripts standalone",
    "config:browser:production": "NODE_ENV=browser:production cozy-scripts --show-config",
    "config:browser:development": "NODE_ENV=browser:development cozy-scripts --show-config",
    "deploy": "git-directory-deploy --username cozy --email contact@cozycloud.cc --directory build/ --repo=https://$GITHUB_TOKEN@github.com/cozy/cozy-contacts.git",
    "test": "jest",
    "stack:docker": "docker run --rm -it -p 8080:8080 -p 5984:5984 -v \"$(pwd)/build\":/data/cozy-app/app cozy/cozy-app-dev",
    "version:manifest": "replace '\\d+\\.\\d+\\.\\d+' $npm_package_version ./manifest.webapp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-contacts.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-contacts/issues"
  },
  "homepage": "https://github.com/cozy/cozy-contacts#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "22.4.4",
    "babel-polyfill": "6.26.0",
    "babel-preset-cozy-app": "0.8.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-react": "7.7.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.4",
    "npm-run-all": "4.1.3",
    "prettier": "1.13.5",
    "pretty-quick": "1.6.0",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-test-renderer": "16.4.0",
    "replace": "1.0.0",
    "stylint": "1.5.9"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "cozy-bar": "5.0.4",
    "cozy-client": "1.0.0-beta.4",
    "cozy-scripts": "0.5.5",
    "cozy-ui": "9.1.0",
    "cozy-vcard": "0.2.18",
    "final-form": "4.8.1",
    "final-form-arrays": "1.0.4",
    "lodash": "4.17.10",
    "preact": "8.2.9",
    "preact-compat": "3.18.0",
    "react-final-form": "3.2.1",
    "react-final-form-arrays": "1.0.6"
  }
}
